{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module":"commonjs",
    "outDir": "dist",
    "paths": {
      "src/*": ["./src/*"],
      "test/*": ["./test/*"],
    },
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "target": "es2016"
  },
  "exclude": ["node_modules", "test/*"]
}
